# Rytr.me API Configuration
# Get your API key from: https://app.rytr.me (Account Settings > API Keys)
VITE_RYTR_API_KEY=your_rytr_api_key_here

# Hunter.io API Configuration
# Get your API key from: https://hunter.io/api_keys
VITE_HUNTERIO_API_KEY=your_hunter_api_key_here

# Mailchimp API Configuration
# Get your API key from: https://admin.mailchimp.com/account/api/
# The server prefix is in your API key after the dash (e.g., us7, us1, etc.)
VITE_MAILCHIMP_API_KEY=your_mailchimp_api_key_here-us1
VITE_MAILCHIMP_SERVER_PREFIX=us1

# Google Analytics Configuration
# Get your tracking ID from: https://analytics.google.com
VITE_GA_TRACKING_ID=G-XXXXXXXXXX
