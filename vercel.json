{
claude/marketing-sales-manager-ui-01G6mu6RC7TBJWSpWdxrwPQ8
  "buildCommand": "cd app && npm install && npm run build",
  "outputDirectory": "app/dist",
  "cleanUrls": true,

  "cleanUrls": false,
main
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/settings",
      "destination": "/scotty/settings.html",
      "permanent": false
    },
    {
      "source": "/dashboard",
      "destination": "/scotty/index.html",
      "permanent": false
    },
    {
      "source": "/agents/:agent",
      "destination": "/scotty/agents/:agent.html",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/scotty/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
